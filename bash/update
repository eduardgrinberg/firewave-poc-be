docker stop flask
docker stop nginx
docker pull eduardgrinberg/firewave-poc-be_flask
docker pull eduardgrinberg/firewave-poc-be_nginx
docker container rm flask
docker container rm nginx
docker run -d --net firewave-poc-be-nt --restart always --name flask eduardgrinberg/firewave-poc-be_flask
docker run -d --net firewave-poc-be-nt --restart always --name nginx -p 5000:80 eduardgrinberg/firewave-poc-be_nginx