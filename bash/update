docker stop flask
docker stop nginx
docker pull docker.io/eduardgrinberg/firewave-poc-be_flask
docker pull docker.io/eduardgrinberg/firewave-poc-be_nginx
docker container rm flask
docker container rm nginx
docker run -d --net firewave-poc-be-nt --restart always --name flask docker.io/eduardgrinberg/firewave-poc-be_flask
docker run -d --net firewave-poc-be-nt --restart always --name nginx -p 5000:80 docker.io/eduardgrinberg/firewave-poc-be_nginx